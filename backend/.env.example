# Logfire Configuration for Docker
# Copy this to .env and update with your actual values

# Database Configuration
EO_CD_DB_HOST=timescaledb
EO_CD_DB_PORT=5432
EO_CD_DB_NAME=eo_db
EO_CD_DB_USER=postgres
EO_CD_DB_PASSWORD=password

# Logfire Configuration
EO_CD_LOGFIRE_TOKEN=your_logfire_token_here
EO_CD_LOGFIRE_PROJECT_NAME=eo-cd-slo
EO_CD_LOGFIRE_ENVIRONMENT=docker
EO_CD_ENABLE_LOGFIRE=true

# Logging Configuration
EO_CD_LOG_LEVEL=INFO

# API Configuration
EO_CD_API_TITLE=Sentinel-2 Image API
EO_CD_API_VERSION=1.0.0
EO_CD_ALLOWED_ORIGINS=["*"]

# Image Processing Configuration
EO_CD_MAX_IMAGE_SIZE=1024
EO_CD_JPEG_QUALITY=85
